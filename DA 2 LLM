   "outputs": [],
   "source": [
    "def measure_response_time(model, prompt):\n",
    "    start = time()\n",
    "    sequences = model(\n",
    "        prompt,\n",
    "        do_sample=True,\n",
    "        top_k=10,\n",
    "        temperature=0.7,\n",
    "        num_return_sequences=1,\n",
    "        eos_token_id=pipeline.tokenizer.eos_token_id,\n",
    "        max_length=512,\n",
    "    )\n",
    "    elapsed_time = time() - start\n",
    "    return sequences[0][\"generated_text\"][len(prompt):], elapsed_time"
   ]
  },
