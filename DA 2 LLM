      "model = LinearRegression()\n",
      "\n",
      "# Fit the model to the data\n",
      "model.fit(X,y)\n",
      "\n",
      "# Print the coefficients\n",
      "print(\"Coefficient of determination: \", model.score(X,y))\n",
      "print(\"Intercept: \", model.intercept_)\n",
